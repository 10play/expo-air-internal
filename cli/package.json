    "localtunnel": "^2.0.2",
    "plist": "^3.1.0",
    "tree-kill": "^1.2.2",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/localtunnel": "^2.0.4",
    "@types/node": "^20.0.0",
    "@types/plist": "^3.0.5",
    "@types/ws": "^8.5.12",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.5.0"
  }
}
