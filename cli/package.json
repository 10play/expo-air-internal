{
  "name": "@10play/expo-air-cli",
  "version": "0.7.1",
  "private": true,
  "type": "module",
  "description": "CLI for expo-air development server",
  "main": "dist/bin/expo-air.js",
  "scripts": {
    "build": "tsc && chmod +x dist/bin/expo-air.js",
    "dev": "tsx bin/expo-air.ts start",
